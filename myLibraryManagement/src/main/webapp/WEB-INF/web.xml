<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">

	<display-name>myLibraryManagement</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>librarymanagment.servlet.RegisterServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/registerUser</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RemoveUserServlet</servlet-name>
		<servlet-class>librarymanagment.servlet.RemoveUserServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RemoveUserServlet</servlet-name>
		<url-pattern>/removeUser</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddBookServlet</servlet-name>
		<servlet-class>librarymanagment.servlet.AddBookServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddBookServlet</servlet-name>
		<url-pattern>/addBook</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RemoveBookServlet</servlet-name>
		<servlet-class>librarymanagment.servlet.RemoveBookServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RemoveBookServlet</servlet-name>
		<url-pattern>/removeBook</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UpdateBookServlet</servlet-name>
		<servlet-class>librarymanagment.servlet.UpdateBookServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UpdateBookServlet</servlet-name>
		<url-pattern>/updateBook</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ViewBookServlet</servlet-name>
		<servlet-class>librarymanagment.servlet.ViewBookServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ViewBookServlet</servlet-name>
		<url-pattern>/viewBooks</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ViewUserServlet</servlet-name>
		<servlet-class>librarymanagment.servlet.ViewUserServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ViewUserServlet</servlet-name>
		<url-pattern>/viewUsers</url-pattern>
	</servlet-mapping>

</web-app>
